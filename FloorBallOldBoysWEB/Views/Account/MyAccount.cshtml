@model MyAccountViewModel

@{
    ViewBag.Title = "Mitt Konto";

}

<h2>Mitt konto</h2>
<div>
    <form method="get" asp-antiforgery="true">
        <div>
            <label>Email: @Model.Email</label>
        </div>
        <div>
            <label>Förnamn: @Model.Firstname</label>
        </div>
        <div>
            <label>Efternamn: @Model.Lastname</label>
        </div>
        <div>
            <label>Personnummer: @Model.SocialSecurityNumber</label>
        </div>
        <div>
            <label>Ort: @Model.City</label>
        </div>
        <div>
            <label>Address: @Model.Street</label>
        </div>
        <div>
            <label>Postnummer: @Model.ZipCode</label>
        </div>
        <div>
            <input type="submit" value="Edit" asp-action="Edit" />
        </div>
    </form>
</div>
