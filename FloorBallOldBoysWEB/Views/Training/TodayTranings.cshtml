@model TrainingsViewModel

@{
    ViewBag.Title = "Dagens träningar";

}

<h2>Dagens träningar</h2>
@if (!Model.Tranings.Any())
{
    <div>
        Ingen träning idag
    </div>
}
else
{
    @Html.Partial("_TrainingSummary",Model)
}
<script type="text/javascript">
    var wasSubmitted = false;
    function checkBeforeSubmit() {
        if (!wasSubmitted) {
            wasSubmitted = true;
            return wasSubmitted;
        }
        return false;
    }
</script>
